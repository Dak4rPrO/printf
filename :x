#include "main.h"
#define BUFFER_SIZE 1024

int _count(va_list list)
{
	int count = 0;
	count++;
	count++;
	return(count);
}

/**
 * get_functions - function
 * @format: format
 */
int (*get_func(char *func))(va_list)
{
	int i = 0;

        op_t op[] = {
                {"c", _printf_c},
                {"s", _printf_s},
                {"d", _printf_d_i},
                {"i", _printf_d_i},
                {"u", _printf_u},
                {"r", _printf_r},
                {"R", _printf_13},
                {"\0", NULL}
        };

	for (i = 0; *op[i].func != '\0'; i++)
	{
		if (*op[i].func == func)
			return (op[i].f);
	}
	_putchar('%');
	_putchar(func);
		return(_count);
}
